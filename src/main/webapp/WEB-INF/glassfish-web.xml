<!DOCTYPE glassfish-web-app PUBLIC "-//GlassFish.org//DTD GlassFish Application Server 3.1 Servlet 3.0//EN" "http://glassfish.org/dtds/glassfish-web-app_3_0-1.dtd">
<glassfish-web-app error-url="">
  <security-role-mapping>
    <role-name>depthead</role-name>
    <group-name>depthead</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>administrator</role-name>
    <group-name>administrator</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>hrrecruiter</role-name>
    <group-name>hrrecruiter</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>employee</role-name>
    <group-name>employee</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>APPLICANT</role-name>
    <group-name>APPLICANT</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>hrassistant</role-name>
    <group-name>hrassistant</group-name>
  </security-role-mapping>
  <security-role-mapping>
    <role-name>hrteamdevelopment</role-name>
    <group-name>hrteamdevelopment</group-name>
  </security-role-mapping>
  <class-loader delegate="true"/>
  <jsp-config>
    <property name="keepgenerated" value="true">
      <description>Keep a copy of the generated servlet class' java code.</description>
    </property>
  </jsp-config>
  <parameter-encoding default-charset="UTF-8"/>
  <property name="alternatedocroot_1" value="from=/images/* dir=c:/var/webapp"/>
</glassfish-web-app>
